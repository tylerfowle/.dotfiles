#!/bin/bash

#set bounds of application window by giving an ApplicationName, width value, and height value
#bounds AppName w h
IFS='%'
appName="$1"
osascript -e 'repeat 4 times
	tell application "System Events"
		try
			tell process '\"$appName\"'
				activate
				set size of window 1 to {'\"$2\"', '\"$3\"'}
			end tell
		end try
	end tell
end repeat
'
